<!--pages/setting/setting.wxml-->
<view class="container">
  <view class="page-label">
    蓝牙设置
  </view>
  <view class="line-point"></view>
  <!-- 指令设置 -->
  <view class="order-view">
    <view class="date-view">
      <text class="title">指令设置</text>
    </view>
    <view class="date-view line-border" bindtap="jumpEditOrderPage">
      <text class="label">修改指令</text>
      <text class="operation">➤</text>
    </view>
    <view class="date-view" bindtap="removeOrder">
      <text class="label">重置指令</text>
      <text class="operation">➤</text>
    </view>
  </view>
  <!-- 其它设置 -->
  <view class="other-view">
    <view class="date-view">
      <text class="title">其它设置</text>
    </view>
    <view class="date-view">
      <text class="label">版本信息(iPad)</text>
      <text class="operation">1.0.0</text>
    </view>
  </view>
</view>
